<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="@dimen/_10sdp"
    android:minWidth="@dimen/_500sdp"
    android:minHeight="@dimen/_150sdp"
    android:orientation="vertical">


    <dreamguys.in.co.gigs.utils.CustomEditext
        android:id="@+id/et_comments"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="top"
        android:hint="@string/hint_feedback"
        android:lines="5" />


    <RatingBar
        android:id="@+id/rb_comments"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:numStars="5" />

    <dreamguys.in.co.gigs.utils.CustomButton
        android:id="@+id/btn_send_feeedback"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="@dimen/_10sdp"
        android:background="@drawable/button_rectangular_shape"
        android:ems="8"
        android:text="Send Feedback"
        android:textColor="@android:color/white" />

</LinearLayout>